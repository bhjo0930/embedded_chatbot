    {
      "parameters": {
        "jsCode": "const items = $input.all();\n\nfor (const item of items) {\n  const collectionName = item.json.category; // Qdrant ì»¬ë ‰ì…˜ ì´ë¦„\n  const filename = item.json.Filename.filename;\n\n  if (!filename) {\n    console.log(\"Filename not found in item. Skipping.\");\n    continue;\n  }\n\n  // 1. filenameìœ¼ë¡œ ê¸°ì¡´ í¬ì¸íŠ¸ ê²€ìƒ‰\n  const searchOptions = {\n    method: 'POST',\n    url: `http://host.docker.internal:6333/collections/${collectionName}/points/scroll`,\n    body: {\n      filter: {\n        must: [{\n          key: \"metadata.filename\",\n          match: {\n            value: filename\n          }\n        }]\n      },\n      limit: 100, // ì¤‘ë³µë  ìˆ˜ ìˆëŠ” ìµœëŒ€ ê°œìˆ˜\n      with_payload: false // idë§Œ í•„ìš”í•˜ë¯€ë¡œ payloadëŠ” ê°€ì ¸ì˜¤ì§€ ì•ŠìŒ\n    },\n    headers: {\n      'Content-Type': 'application/json',\n    },\n    json: true // ìë™ìœ¼ë¡œ JSONì„ íŒŒì‹±\n  };\n\n  const searchResponse = await this.helpers.httpRequest(searchOptions);\n  const pointsToDelete = searchResponse.result.points;\n\n  // 2. ê¸°ì¡´ í¬ì¸íŠ¸ê°€ ìˆìœ¼ë©´ ì‚­ì œ\n  if (pointsToDelete && pointsToDelete.length > 0) {\n    const pointIds = pointsToDelete.map(point => point.id);\n    console.log(`Found ${pointIds.length} existing points to delete for filename: ${filename}`);\n\n    const deleteOptions = {\n      method: 'POST',\n      url: `http://host.docker.internal:6333/collections/${collectionName}/points/delete`,\n      body: {\n        points: pointIds\n      },\n      headers: {\n        'Content-Type': 'application/json',\n      },\n      json: true\n    };  \n    \n    await this.helpers.httpRequest(deleteOptions);\n    console.log(`Successfully deleted points with IDs: ${pointIds.join(', ')}`);\n  } else {\n    console.log(`No existing points found for filename: ${filename}. Proceeding to insert.`);\n  }\n}\n\n// 3. ë‹¤ìŒ ë…¸ë“œë¡œ ì›ë³¸ ì•„ì´í…œ ì „ë‹¬\nreturn items;"
